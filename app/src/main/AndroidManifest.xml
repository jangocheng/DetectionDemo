<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.compilesense.liuyi.detectiondemo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RecognitionActivity" />
        <activity
            android:name=".activity.PersonManageActivity"
            android:label="@string/title_activity_person_manage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.FaceManageActivity"
            android:label="@string/title_activity_face_manage"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>